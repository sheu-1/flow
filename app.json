{
  "expo": {
    "name": "Cashflow Tracker",
    "slug": "cashflow-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexsheunda.cashflowtracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#121212"
      },
      "permissions": [
        "READ_SMS",
        "RECEIVE_SMS"
      ],
      "package": "com.alexsheunda.cashflowtracker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store",
      "expo-font"      
    ],
    "extra": {
      "eas": {
        "projectId": "a47063e7-114d-4c07-969f-49369d9739db"
      }
    }
  }
}
